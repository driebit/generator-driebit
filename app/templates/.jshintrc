{
    "bitwise": true,
    "camelCase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noempty": true,
    "nonew": false,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,

    "asi": false,
    "boss": false,
    "lastsemic": true,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": false,
    "shadow": true,

    <% if (type == 'browser') { %>
        "es3": true,
        "node": false,
        "browser": true

        <% if (jQuery) {%>
            ,
            "jquery': true
        <% } %>

    <% } else { %>
        "es3": false,
        "node": true
    <% } %>
}
